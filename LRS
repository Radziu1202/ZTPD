zadanie 1
A:

CREATE TABLE A6_LRS (
    GEOM SDO_GEOMETRY
);

B:

INSERT INTO A6_LRS
SELECT s.GEOM
FROM STREETS_AND_RAILROADS s WHERE s.ID = (
    SELECT s2.ID
    FROM STREETS_AND_RAILROADS s2, MAJOR_CITIES c
    WHERE SDO_RELATE(s2.GEOM,SDO_GEOM.SDO_BUFFER(c.GEOM, 10, 1, 'unit=km'), 'mask=ANYINTERACT') = 'TRUE AND c.CITY_NAME = 'Koszalin');
